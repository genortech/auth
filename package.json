{
  "name": "auth",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "npm run test:integration && npm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "generate": "drizzle-kit generate:pg --config=drizzle.config.ts",
    "push": "drizzle-kit push:pg --config=drizzle.config.ts",
    "migrate": "tsx migrate.ts"
  },
  "devDependencies": {
    "@lucia-auth/adapter-postgresql": "^2.0.0",
    "@playwright/test": "^1.36.2",
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/kit": "^1.22.3",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "dotenv": "^16.3.1",
    "drizzle-kit": "^0.19.12",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-svelte": "^2.32.4",
    "prettier": "^3.0.0",
    "prettier-plugin-svelte": "^3.0.2",
    "svelte": "^4.1.1",
    "svelte-check": "^3.4.6",
    "sveltekit-superforms": "^1.5.0",
    "tslib": "^2.6.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.7",
    "vitest": "^0.33.0",
    "ws": "^8.13.0",
    "zod": "^3.21.4"
  },
  "type": "module",
  "dependencies": {
    "@lucia-auth/oauth": "^2.0.0",
    "@neondatabase/serverless": "^0.5.6",
    "drizzle-orm": "^0.27.2",
    "lucia": "^2.0.0"
  }
}
